  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/tests/*.test.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "clean-css-cli": "^5.6.3",
    "cypress": "^15.1.0",
    "javascript-obfuscator": "^0.6.2",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "terser": "^5.44.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "astro": "^5.14.5"
  }
}
