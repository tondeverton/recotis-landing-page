{
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "clean": "rm *.min.js && rm *.min.css",
    "unit:test": "jest",
    "e2e:test": "cypress run --config-file cypress/cypress.config.js",
    "test": "npm run build && npm run unit:test && npm run e2e:test"
